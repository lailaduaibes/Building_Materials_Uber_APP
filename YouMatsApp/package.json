{
  "name": "youmats-delivery-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "tunnel": "expo start --tunnel",
    "eject": "expo eject",
    "test": "jest",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "i18n:scan": "i18next-scanner --config i18next-scanner.config.js",
    "i18n:extract": "npm run i18n:scan",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:report": "npm run i18n:validate",
    "translate:missing": "npm run i18n:scan && echo 'Translation keys extracted! Check translation files for missing values.'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.55.0",
    "expo": "~53.0.20",
    "expo-device": "~7.1.4",
    "expo-document-picker": "^13.1.6",
    "expo-font": "~13.3.2",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-localization": "^16.1.6",
    "expo-location": "^18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.0",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-task-manager": "^13.1.6",
    "i18next": "^25.4.2",
    "react": "19.0.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "^1.15.6",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "expo-dev-client": "~5.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "i18next-scanner": "^4.6.0",
    "jest": "^29.7.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "react-native"
